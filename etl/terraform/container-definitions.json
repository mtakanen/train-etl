[
    {
      "name": "TrainETL-Container",
      "image": "AWS_ACCOUNT_ID.dkr.ecr.AWS_REGION.amazonaws.com/train-etl:latest",
      "entryPoint": ["python", "etl.py", "--train_number=8322", "--weeks=10"],
      "cpu": 256,
      "memory": 512,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "trainetl-logs",
          "awslogs-region": "$AWS_REGION",
          "awslogs-stream-prefix": "awslogs-trainetl-service"
        }
      },
      "essential": true
    }
]
